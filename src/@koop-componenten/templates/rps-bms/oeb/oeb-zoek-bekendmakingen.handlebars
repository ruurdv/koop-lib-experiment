
<div class="container row" id="content" role="main">
  <div class="row">

    
    {{#> @jumbotron jumbotronContext }}
      <form class="form" data-decorator="init-form-validation">
        
        <div class="form__row form__row--medium columns columns--dominant">
          
          <div class="column">

            <div class="form__row form__row--large">
              <fieldset>
                <legend>Documentsoort</legend>
                <div class="form__row">
                  <div data-decorator="init-unselectall" data-config='{"checkboxSelector":".accordion__item__header-checkboxtrigger"}'>
                    {{#> @accordion accordion1 }}
                      {{#> @accordion-item item1 }}
                      {{/@accordion-item}}
                      {{#> @accordion-item item2 }}
                          <fieldset>
                            <legend class="visually-hidden">Tractatenblad</legend>
                            <fieldset>
                              <legend>Tractatenblad</legend>
                              <div class="columns">
                                <div class="column">
                                  <div class="form__row">
                                    {{ render "@input-text" trac_jaargang }}
                                  </div>
                                </div>
                                <div class="column">
                                  <div class="form__row">
                                    {{ render "@input-text" trac_nummer }}
                                  </div>
                                </div>
                              </div>
                              <div class="form__row">
                                <p class="form__sublegend form__sublegend--secondary">Onderwerp(en)</p>
                                
                              </div>
                            </fieldset>
                            <fieldset>
                              <legend>Systematisch</legend>
                              <div class="columns">
                                <div class="column">
                                  <div class="form__row">
                                    {{ render "@input-text" trac_nummer2 }}
                                  </div>
                                </div>
                                <div class="column">
                                  <div class="form__row">
                                    {{ render "@input-text" trac_jaar }}
                                  </div>
                                </div>
                              </div>
                              <div class="form__row">
                                {{ render "@input-text" trac_volgnummer }}
                              </div>
                            </fieldset>
                            <fieldset>
                              <legend>Totstandkoming verdrag</legend>
                              <div class="columns">
                                <div class="column">
                                  <div class="form__row">
                                    
                                  </div>
                                </div>
                                <div class="column">
                                  <div class="form__row">
                                    
                                  </div>
                                </div>
                              </div>
                              <div class="form__row">
                                {{ render "@input-text" trac_plaats }}
                              </div>
                            </fieldset>
                          </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item3 }}
                        <fieldset>
                          <legend>Staatsblad</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" sb_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" sb_nummer }}
                              </div>
                            </div>
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item4 }}
                        <fieldset>
                          <legend>Staatscourant</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" sc_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" sc_nummer }}
                              </div>
                            </div>
                          </div>
                          <div class="form__row">
                            <p class="form__sublegend form__sublegend--secondary">Rubriek(en)</p>
                            
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item5 }}
                        <fieldset>
                          <legend>Gemeenteblad</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" gm_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" gm_nummer }}
                              </div>
                            </div>
                          </div>
                          <div class="form__row">
                            <p class="form__sublegend form__sublegend--secondary">Rubriek(en)</p>
                            
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item6 }}
                        <fieldset>
                          <legend>Provinciaal blad</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" pb_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" pb_nummer }}
                              </div>
                            </div>
                          </div>
                          <div class="form__row">
                            <p class="form__sublegend form__sublegend--secondary">Rubriek(en)</p>
                            
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item7 }}
                        <fieldset>
                          <legend>Waterschapsblad</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" wb_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" wb_nummer }}
                              </div>
                            </div>
                          </div>
                          <div class="form__row">
                            <p class="form__sublegend form__sublegend--secondary">Rubriek(en)</p>
                            
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                      {{#> @accordion-item item8 }}
                        <fieldset>
                          <legend>Blad gemeentelijke regeling</legend>
                          <div class="columns">
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" bgr_jaargang }}
                              </div>
                            </div>
                            <div class="column">
                              <div class="form__row">
                                {{ render "@input-text" bgr_nummer }}
                              </div>
                            </div>
                          </div>
                          <div class="form__row">
                            <p class="form__sublegend form__sublegend--secondary">Rubriek(en)</p>
                            
                          </div>
                        </fieldset>
                      {{/@accordion-item}}
                    {{/@accordion}}
                  </div>
                </div>
              </fieldset>
            </div>
          </div>
        </div>
        
      </form>
    {{/@jumbotron}}

  </div>

</div>
